   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/cookie-parser": "^1.4.2",
      "@types/express": "^4.17.13",
      "@types/jest": "^27.4.1",
      "@types/jsonwebtoken": "^8.5.8",
      "@types/node": "^16.11.10",
      "@types/supertest": "^2.0.12",
      "@typescript-eslint/eslint-plugin": "^5.18.0",
      "@typescript-eslint/parser": "^5.18.0",
      "codecov": "^3.8.3",
      "concurrently": "^7.1.0",
      "dotenv": "^16.0.0",
      "eslint": "^7.32.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-prettier": "^4.0.0",
      "jest": "^27.5.1",
      "nodemon": "^2.0.15",
      "prettier": "^2.6.2",
      "supertest": "^6.2.2",
      "ts-jest": "^27.1.4"
   },
   "scripts": {
      "start": "node build/app.js",
      "build": "tsc -p .",
      "start:prod": "yarn build && yarn start",
      "backend": "nodemon --watch \"**/*.ts\" --exec \"ts-node --files\" src/app.ts",
      "frontend": "yarn --cwd client start",
      "dev": "concurrently \"yarn backend\" \"yarn --cwd client start\"",
      "typeorm": "typeorm-ts-node-commonjs",
      "lint": "eslint .",
      "test": "NODE_ENV=test jest --setupFiles dotenv/config --forceExit --detectOpenHandles --coverage",
      "test:win": "SET NODE_ENV=test& jest --setupFiles dotenv/config --forceExit --detectOpenHandles --coverage"
   }
